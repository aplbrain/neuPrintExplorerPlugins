  "homepage": "https://github.com/connectome-neuprint/neuPrintExplorerPlugins#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
