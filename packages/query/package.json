{
  "name": "@neuprint/queries",
  "version": "9.1.6",
  "description": "Query plugins used by the neuPrintExplorer website",
  "main": "build/index.js",
  "scripts": {
    "pretest": "eslint --ext .jsx,.js src/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-debug": "node --inspect node_modules/.bin/jest --runInBand",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production npx babel ./src --out-dir ./build --ignore '**/*.test.jsx' --ignore '**/*.test.js'",
    "dev": "NODE_ENV=development npx babel ./src --out-dir ./build --ignore '**/*.test.jsx' --ignore '**/*.test.js' --source-maps --watch --verbose",
